{
  "timeout": 10000,
  "events": [
    {
      "type": "click",
      "polling": false,
      "attributes": ["clientX", "clientY"]
    },
    {
      "type": "scroll",
      "polling": true,
      "interval": 1000
    },
    {
      "type": "keydown",
      "polling": false,
      "attributes": ["key", "code"]
    },
    {
      "type": "mousemove",
      "polling": true,
      "interval": 100,
      "attributes": ["screenX", "screenY"]
    }
  ]
}